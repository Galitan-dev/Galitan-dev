{
  "name": "my-portfolio",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.7.0",
    "@japa/preset-adonis": "^1.0.16",
    "@japa/runner": "^2.0.8",
    "@symfony/webpack-encore": "^2.1.0",
    "@types/p5": "^1.4.2",
    "adonis-preset-ts": "^2.1.0",
    "bulma": "^0.9.4",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "less": "^4.1.2",
    "less-loader": "^10",
    "pino-pretty": "^7.6.1",
    "postcss-loader": "^6",
    "prettier": "^2.6.2",
    "sass": "^1.52.2",
    "sass-loader": "^12",
    "ts-loader": "^9.3.0",
    "typescript": "~4.6",
    "vue": "^3.2.33",
    "vue-loader": "^17.0.0",
    "vue-router": "4",
    "youch": "^3.2.0",
    "youch-terminal": "^2.1.4"
  },
  "dependencies": {
    "@adonisjs/core": "^5.8.0",
    "@adonisjs/repl": "^3.1.0",
    "@adonisjs/session": "^6.2.0",
    "@adonisjs/shield": "^7.0.0",
    "@adonisjs/view": "^6.1.0",
    "@vue/compiler-sfc": "^3.2.36",
    "p5": "^1.4.1",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21"
  }
}
